//
// this ESLINT config file augments the existing **leading config** in package.json 
// for the sole purpose of making `npm run fix` pass at all times, due to `eslint`
// **not** barfing a hairball because there's an odd bit of code still lingering in 
// there while we specifically want eslint to 'fix' everything in the code that's
// possible to fix!
//
// Hence this config only serves to force any eslint to become **warnings** instead.
//
{
rules: {
	camelcase: 1,
	eqeqeq: 1,
	vars-on-top: 1,
	no-restricted-properties: 1,
	no-var: 1,
	no-useless-escape: 1,
	prefer-const: 1,
	no-eval: 1,	
	no-shadow: 1,
	no-new: 1,
	default-case: 1,
	"import/no-amd": 1,
	"import/no-dynamic-require": 1,
	"import/no-unresolved": 1,
	"import/no-mutable-exports": 1,
	"import/extensions": 1,
	no-prototype-builtins: 1,
	no-cond-assign: 1,
	no-return-assign: 1,
	no-continue: 1,
	no-labels: 1,
	no-empty: 1,
	no-tabs: 1,
	block-scoped-var: 1,
	no-this-before-super: 1,
	no-throw-literal: 1,
	no-self-compare: 1,
	class-methods-use-this: 1,
	no-redeclare: 1,
	indent: 1,
	no-proto: 1,
	no-inner-declarations: 1,
	getter-return: 1,
}}